model Account {
  id String @id @default(uuid())

  type     String
  provider String

  refreshToken String?
  accessToken  String?
  expiresAt    DateTime?

  user   User?   @relation(fields: [userId], references: [id])
  userId String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt()

  @@map("accounts")
}
